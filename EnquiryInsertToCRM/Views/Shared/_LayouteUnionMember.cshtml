    <style>
        .chk-DirectOffers label {
            position: relative;
            display: inline-block;
            padding-left: 5px;
        }

        .rdo-label {
            font-weight: normal;
        }

        .chk-DirectOffers input {
            margin-top: 8px;
        }

        .chk-DirectOffers {
            padding-left: 30px;
            margin-bottom: 0;
            cursor: pointer;
        }

        .post-content--body label, .field-validation-error span {
            font-size: 14px;
        }

        .form-submit-button {
            border: 1px solid #ccc;
            padding: 6px 12px;
            color: #666;
            text-shadow: 0 1px #fff;
            cursor: pointer;
            border-radius: 3px 3px;
            box-shadow: 0 1px #fff inset, 0 1px #ddd;
            background: #f5f5f5;
            background: -moz-linear-gradient(top, #f5f5f5 0%, #eeeeee 100%);
            background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f5f5f5), color-stop(100%,#eeeeee));
            background: -webkit-linear-gradient(top, #f5f5f5 0%,#eeeeee 100%);
            background: -o-linear-gradient(top, #f5f5f5 0%,#eeeeee 100%);
            background: -ms-linear-gradient(top, #f5f5f5 0%,#eeeeee 100%);
            background: linear-gradient(to top, #f5f5f5 0%, #eeeeee 100%);
            filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f5f5f5', endColorstr='#eeeeee',GradientType=0 )
        }

        .form-submit-button {
            color: #333;
            font-size: 1em;
            margin-top: 12px;
            margin-bottom: 12px;
            font-size: 14px;
        }

        .warnig-note {
            background-repeat: no-repeat;
            color: #000;
            font-size: 13px;
            position: fixed;
            border-top: 0px;
            left: 0;
            right: 0;
            z-index: 999;
            padding: 8px 14%;
            background-size: cover;
            background-color: #f1c343;
            top: 0;
            text-align: center;
        }

        .form-submit-button:active {
            background: #eeeeee;
            background: -moz-linear-gradient(top, #eeeeee 0%, #f5f5f5 100%);
            background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#eeeeee), color-stop(100%,#f5f5f5));
            background: -webkit-linear-gradient(top, #eeeeee 0%,#f5f5f5 100%);
            background: -o-linear-gradient(top, #eeeeee 0%,#f5f5f5 100%);
            background: -ms-linear-gradient(top, #eeeeee 0%,#f5f5f5 100%);
            background: linear-gradient(top, #eeeeee 0%,#f5f5f5 100%);
            filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#eeeeee', endColorstr='#f5f5f5',GradientType=0 );
            -moz-box-shadow: 0 1px #fff inset;
            -webkit-box-shadow: 0 1px #fff inset;
            box-shadow: 0 1px #fff inset;
        }

        .form-submit-button:hover {
            background: #ffffff;
            background: -moz-linear-gradient(top, #ffffff 0%, #f5f5f5 100%);
            background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ffffff), color-stop(100%,#f5f5f5));
            background: -webkit-linear-gradient(top, #ffffff 0%,#f5f5f5 100%);
            background: -o-linear-gradient(top, #ffffff 0%,#f5f5f5 100%);
            background: -ms-linear-gradient(top, #ffffff 0%,#f5f5f5 100%);
            background: linear-gradient(top, #ffffff 0%,#f5f5f5 100%);
            filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#f5f5f5',GradientType=0 );
        }

        .post-content h1 {
            color: #000 !important;
            font-size: 22px !important;
            letter-spacing: 0px !important;
        }

        .font-normal {
            font-weight: 400;
            cursor: pointer;
            color: #696868;
        }
        .page-width__content .post-content a {
        text-decoration:none !important;
        }
    </style>
    <link href="/Content/chosen.css" rel="stylesheet" />
