<body>
    <div>
        @RenderBody()
    </div>
    @Scripts.Render("~/bundles/bootstrap")
    <script src="/Scripts/chosen.jquery.min.js"></script>
    <!--Validation js-->
    <script src="/Scripts/jquery-migrate-1.2.1.min.js"></script>
    <script src="https://ajax.aspnetcdn.com/ajax/jquery.validate/1.13.0/jquery.validate.min.js" type="text/javascript"></script>
    <script src="/Scripts/jquery.validate.unobtrusive.min.js" type="text/javascript"></script>
    <script type="text/javascript" src="https://ajax.aspnetcdn.com/ajax/mvc/3.0/jquery.unobtrusive-ajax.min.js"></script>
    <script type="text/javascript" src="https://cdn.jsdelivr.net/momentjs/latest/moment.min.js"></script>
    <script>
        $(document).ready(function () {   

            $(document).ready(function () {
                $('.chosen-select').chosen({ 'width': '100%', 'white-space': 'nowrap' });
                $("#UnionID").css("display", "block");
                $("#UnionID").css("height", "0");
                $("#UnionID").css("padding", "0");
                $("#UnionID").css("visibility", "hidden");
            });
            if ($("#IsInvalidClientId").val() === "True") {
                $(".warnig-note").show();
            }  
            $('.chosen-search input[type=\"text\"]').keyup(function (e) {
                if ($(".chosen-search input[type=\"text\"]").val().length < 3) {
                    $('.chosen-results').hide();
                    $('.chosen-results').show();
                    $('.chosen-results').html('');
                    $('.chosen-results').html('<li class="no-results">No results match "<span>' + $(".chosen-search input[type=\"text\"]").val() + '</span></li>')
                } else {
                    $('.chosen-results').show();
                }
            }); 
            $("#ckbCheckAll").click(function () {
                $(".chk-all").prop('checked', $(this).prop('checked'));
                $(".chk-DirectOffers").trigger("click");
            });
            $(".chk-DirectOffers").click(function () {  
                if ($('.chk-all:checked').length == $('.chk-all').length) {
                    $("#ckbCheckAll")[0].checked = true; //change "select all" checked status to true
                } else {
                    $("#ckbCheckAll")[0].checked = false; 
                }
                var arrayDirectOffersId = []
                var arrayDirectOffersName = []
                $("input:checkbox[name=chkDirectOffers]:checked").each(function () {
                    arrayDirectOffersId.push($(this).val());
                    arrayDirectOffersName.push($("label[for='" + $(this).val() + "']").text());
                });
                $("#DirectOffers").val(arrayDirectOffersId.join(','));
                $("#strDirectOffersName").val(arrayDirectOffersName.join(','));
            });
            $("#UnionID").change(function () {
                $("#UnionName").val("");
                if ($('option:selected', this).text() !== "-- Select --") {
                    $("#UnionName").val(($('option:selected', this).text()));
                }
            });
            $("#btnBack").click(function () {
                $('#UnionMemberForm').show();
                $('#divResponse').hide();
                $("#lblmsg").empty();
                $("#btnBack").hide();
            });
        });
        $(window).on("load", function () {
            $('#UnionID').trigger('change');
            $(".chk-DirectOffers").trigger('click');
        });
        $('.scpace-notallow').on('keypress', function (e) {
            if (e.which == 32)
                return false;
        });
        function OnBeginUnionMemberForm() {            
            $("#divResponse").hide();
            $("#lblmsg").empty();
            $('#btnsubmit').hide();
            $('#spnImgProcessing').show();
        }
        function OnSuccessUnionMemberForm(res) {
            $("#UnionMemberForm").hide();
            $("#divResponse").show();
            $("#lblmsg").empty();
            $('#spnImgProcessing').hide();
            $("#h1Response").empty();
            $("#h1Response").text("Thank You!");
            $('#btnsubmit').show();
            $("#btnBack").hide();
            if (res == "success") {
                $("#ImgSrcResponse").attr("src", "/images/check-icon.png");
                $("#lblmsg").text("Your data has been successfully submitted in CRM.");
                $('#UnionMemberForm')[0].reset();
            } else if (res == "successupdated") {
                $("#ImgSrcResponse").attr("src", "/images/check-icon.png");
                $("#lblmsg").text("Your data has been successfully updated in CRM");
                $('#UnionMemberForm')[0].reset();
            } else if (res == "abentryError") {
                $("#h1Response").empty();
                $("#ImgSrcResponse").attr("src", "/images/error-icon.png");
                $("#lblmsg").text("Some internal error occurred! During CRM process.");
                $('#UnionMemberForm')[0].reset();
            } else if (res == "noteError") {                
                $("#h1Response").empty();
                $("#ImgSrcResponse").attr("src", "/images/error-icon.png");
                $("#lblmsg").text("Some internal error occurred! During note process.");
                $('#UnionMemberForm')[0].reset();
            } else if (res == "invalidformdata") {                
                $("#btnBack").show();
                $("#h1Response").empty();
                $("#ImgSrcResponse").attr("src", "/images/error-icon.png");
                $("#lblmsg").text("Paased invalid form data.");
                //$('#UnionMemberForm')[0].reset();
            } else {  
                $("#h1Response").empty();
                $("#ImgSrcResponse").attr("src", "/images/error-icon.png");
                $("#lblmsg").text("Some internal error occurred.");
                $('#UnionMemberForm')[0].reset();                
            }
        }
        function OnFailureUnionMemberForm() {
            $("#ImgSrcResponse").attr("src", "/images/error-icon.png");
            $("#h1Response").empty();
            $("#UnionMemberForm").hide();
            $('#btnsubmit').show();
            $('#spnImgProcessing').hide();
            $("#divResponse").show();
            $("#lblmsg").text("Some internal error occurred.");
        }

    </script>
</body>
</html>
